{
  "name": "blode-icons-react",
  "version": "0.3.1",
  "description": "Blode icons for React â€” drop-in lucide-react replacement",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./icons/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./dynamic": {
      "types": "./dist/dynamic.d.ts",
      "import": "./dist/dynamic.js"
    },
    "./dynamicIconImports": {
      "types": "./dist/dynamicIconImports.d.ts",
      "import": "./dist/dynamicIconImports.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "node scripts/build.mjs",
    "prepublishOnly": "node scripts/build.mjs",
    "check:types": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --fix .",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "component",
    "ui",
    "lucide",
    "lucide-react",
    "blode"
  ],
  "author": "Matthew Blode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mblode/blode-icons/issues"
  },
  "homepage": "https://github.com/mblode/blode-icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mblode/blode-icons.git",
    "directory": "packages/blode-icons-react"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.2.79",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "^5.4.0",
    "ultracite": "^7.2.3"
  }
}
